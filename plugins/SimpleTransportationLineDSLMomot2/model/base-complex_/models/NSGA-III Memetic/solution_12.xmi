<?xml version="1.0" encoding="ASCII"?>
<simpleTransportationLineDSL:System xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:simpleTransportationLineDSL="http://www.example.org/simpleTransportationLineDSL">
  <simconfig budget="100000"/>
  <area name="mainArea">
    <component xsi:type="simpleTransportationLineDSL:SentStore" typeId="9" processingTime="0" capacity="9999">
      <inputSlot requiredType="3" name="input"/>
      <inputSlot outputSlot="//@area.0/@component.3/@outputSlot.1" requiredType="3" name="input2"/>
    </component>
    <component xsi:type="simpleTransportationLineDSL:WasteStore" typeId="10" processingTime="0" capacity="9999">
      <inputSlot name="input"/>
      <inputSlot name="input2"/>
    </component>
    <component xsi:type="simpleTransportationLineDSL:Machine" pseudoId="9" typeId="3" processingTime="20000" activeEnergy="500" passiveEnergy="50" cost="5000" minInput="2">
      <outputSlot inputSlot="//@area.0/@component.3/@inputSlot.0" providedType="3" name="output"/>
      <inputSlot outputSlot="//@area.0/@component.5/@outputSlot.0" requiredType="2" name="input1"/>
      <inputSlot outputSlot="//@area.0/@component.4/@outputSlot.0" requiredType="2" name="input2"/>
    </component>
    <component xsi:type="simpleTransportationLineDSL:Conveyor" pseudoId="10" typeId="2" processingTime="2000" activeEnergy="200" passiveEnergy="150" cost="150">
      <outputSlot providedType="3" name="output1"/>
      <outputSlot inputSlot="//@area.0/@component.0/@inputSlot.1" providedType="3" name="output2"/>
      <inputSlot outputSlot="//@area.0/@component.2/@outputSlot.0" requiredType="3" name="input1"/>
      <inputSlot requiredType="3" name="input2"/>
    </component>
    <component xsi:type="simpleTransportationLineDSL:ItemGenerator" pseudoId="1" typeId="1" processingTime="60000" activeEnergy="200" passiveEnergy="10" productionCost="45" cost="2000" generatedType="2">
      <outputSlot inputSlot="//@area.0/@component.2/@inputSlot.1" providedType="2" name="output"/>
    </component>
    <component xsi:type="simpleTransportationLineDSL:ItemGenerator" pseudoId="10" typeId="1" processingTime="60000" activeEnergy="200" passiveEnergy="10" productionCost="45" cost="2000" generatedType="2">
      <outputSlot inputSlot="//@area.0/@component.2/@inputSlot.0" providedType="2" name="output"/>
    </component>
  </area>
</simpleTransportationLineDSL:System>
